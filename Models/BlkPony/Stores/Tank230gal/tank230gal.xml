<?xml version="1.0"?>

<!-- 230 gallon external fuel tank wrapper, by Dave Culp -->

<PropertyList>

 <path>tank230gal.ac</path>

 <animation>
  <type>select</type>
  <object-name>tank</object-name>
  <condition>
   <not>
     <property>controls/armament/station3</property>
   </not>
  </condition>
 </animation>


 <!-- *** LoD *** -->
 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>12000</max-m>
 </animation>

</PropertyList>
