<?xml version="1.0"?>

<!-- OV10 animation by Jim Wilson -->
<!-- Control surface movements, gear retract by Jens Thoms Toerring -->
<!-- Roll spoilers, 3D panel by Dave Culp -->

<PropertyList>

 <path>OV10_NATO.ac</path>

 <nasal>
   <load>
      var self = cmdarg();
      var float_0 = self.getNode("sim/multiplay/generic/float[0]", 1);
      var float_1 = self.getNode("sim/multiplay/generic/float[1]", 1);
      var float_2 = self.getNode("sim/multiplay/generic/float[2]", 1);
      var float_3 = self.getNode("sim/multiplay/generic/float[3]", 1);
	  
      var int_0 = self.getNode("sim/multiplay/generic/int[0]", 1);


      var gear_0_steering = self.getNode("gear/gear[0]/steering-norm", 1);
      var gear_0_rollspeed = self.getNode("gear/gear[0]/rollspeed-ms", 1);
      var gear_1_rollspeed = self.getNode("gear/gear[1]/rollspeed-ms", 1);
      var gear_2_rollspeed = self.getNode("gear/gear[2]/rollspeed-ms", 1);
      var smoke = self.getNode("controls/smoke", 1);
      	  
      gear_0_steering.alias(float_0);
      gear_0_rollspeed.alias(float_1);
      gear_1_rollspeed.alias(float_2);
      gear_2_rollspeed.alias(float_3);
	  smoke.alias(int_0);
	  
      var undercarriage = setlistener(self.getNode("sim/multiplay/generic/int[1]",1), func() {
	    var int1 = self.getNode("sim/multiplay/generic/int[1]").getValue();
		self.getNode("controls/armament/station1").setBoolValue(!bits.test(int1,0));
		self.getNode("controls/armament/station2").setBoolValue(!bits.test(int1,1));
		self.getNode("controls/armament/station3").setBoolValue(!bits.test(int1,2));
		self.getNode("controls/armament/station4").setBoolValue(!bits.test(int1,3));
		self.getNode("controls/armament/station5").setBoolValue(!bits.test(int1,4));		
		}, 1, 0);
	</load>

  <unload>
      gear_0_steering.unalias();
      gear_0_rollspeed.unalias();
      gear_1_rollspeed.unalias();
      gear_2_rollspeed.unalias();
      smoke.unalias();
	  removelistener(undercarriage);
  </unload>
 </nasal>
 
