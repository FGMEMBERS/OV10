<?xml version="1.0"?>

<!-- OV10 animation by Jim Wilson -->
<!-- Control surface movements, gear retract by Jens Thoms Toerring -->
<!-- Roll spoilers by Dave Culp -->

<PropertyList>

 <path>OV10.ac</path>

 <offsets>
   <z-m>0.90</z-m>
   <pitch-deg>-1.0</pitch-deg>
 </offsets>  

 <animation>
  <type>select</type>
  <object-name>PropLeft</object-name>
  <condition>
   <less-than>
    <property>engines/engine[0]/n2</property>
    <value>2</value>
   </less-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>PropRight</object-name>
  <condition>
   <less-than>
    <property>engines/engine[1]/n2</property>
    <value>2</value>
   </less-than>
  </condition>
 </animation>


 <animation>
  <type>select</type>
  <object-name>PropBlurLeft</object-name>
  <condition>
   <and>
    <greater-than>
     <property>engines/engine[0]/n2</property>
     <value>2</value>
    </greater-than>
    <less-than>
     <property>engines/engine[0]/n2</property>
     <value>30</value>
    </less-than>
   </and>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>PropBlurRight</object-name>
  <condition>
   <and>
    <greater-than>
     <property>engines/engine[1]/n2</property>
     <value>2</value>
    </greater-than>
    <less-than>
     <property>engines/engine[1]/n2</property>
     <value>30</value>
    </less-than>
   </and>
  </condition>
 </animation>

 <animation>
  <type>select</type>
